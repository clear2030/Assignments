import random

def main():
  print("Starting The Code Challenge")
  # Declare Any main() Variables - (Not Global)

  print("Starting 8.1.2.4.3 - Python - Ch 2 - W3-Schools Review - Flow Control - While Loops - ğŸ“Ÿ Coding / ğŸ› ï¸ LAB Activity / ğŸš€ Quiz - âš“ Of Code Challenge")
