  i = 1
  while i < 6:
      print(i)
      i += 1
      
  i = 1
  while i < 6:
      print(i)
      if i == 3:
          break
      i += 1
      
  i = 0
  while i < 6:
      i += 1
      if i == 3:
          continue
      print(i)
      
  i = 1
  while i < 6:
      print(i)
      i += 1
  else:
      print("i is no longer less than 6")
      
  rand_num = random.randint(0,10)
  y = -1
  while y != rand_num:
      y = int(input("Guess the number: "))
      if(y<rand_num):
          print("Your number is too low, try again.")
          continue
      if(y>rand_num):
          print("Your number is too high, try again.")
          continue
      break
  else:
      print("You got it!")
          
  print()
  print("Ending 8.1.2.4.3 - Python - Ch 2 - W3-Schools Review - Flow Control - While Loops - ğŸ“Ÿ Coding / ğŸ› ï¸ LAB Activity / ğŸš€ Quiz - âš“ Of Code Challenge")
  print("Ending The Code Challenge")

main()

