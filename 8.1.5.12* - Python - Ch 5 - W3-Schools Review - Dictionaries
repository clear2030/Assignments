  thisdict = {
  "brand": "Ford",
  "model": "Mustang",
  "year": 1964
  }
  print(thisdict)
  print(thisdict["brand"])
  thisdict["year"] = "2025"
  print(len(thisdict))
  print(type(thisdict))
  print()
  
  thisdict = dict(name = "John", age = 36, country = "Norway")
  print(thisdict)
  print()
  
  thisdict = dict(restaurant = "Mcdonalds", order = "Big Mac", price = 0)
  for i in thisdict:
      print(i, ": ", thisdict[i], sep = "")
  
  print()
  print("Ending 8.1.5.12* - Python - Ch 5 - W3-Schools Review - Dictionaries - 🛠️ LAB Activity / 🚀 Quiz - ✔️ Mastery Path Of Code Challenge")
